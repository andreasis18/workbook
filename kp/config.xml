<?xml version='1.0' encoding='utf-8'?>
<widget id="id.ac.ubaya.gpb" version="10.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Workbook GPB</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="21  " />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
        <preference name="AndroidExtraFilesystems" value="sdcard,cache" />
        <allow-intent href="market:*" />
        <icon platform="android" qualifier="ldpi" src="www/icon/android/icon-36-ldpi.png" />
        <icon platform="android" qualifier="mdpi" src="www/icon/android/icon-48-mdpi.png" />
        <icon platform="android" qualifier="hdpi" src="www/icon/android/icon-72-hdpi.png" />
        <icon platform="android" qualifier="xhdpi" src="www/icon/android/icon-96-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
        <icon height="57" platform="ios" src="www/icon/ios/icon-57.png" width="57" />
        <icon height="114" platform="ios" src="www/icon/ios/icon-57-2x.png" width="114" />
        <icon height="72" platform="ios" src="www/icon/ios/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="www/icon/ios/icon-72-2x.png" width="144" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="permissions" value="true" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-fileopener" spec="^1.0.5" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
</widget>
